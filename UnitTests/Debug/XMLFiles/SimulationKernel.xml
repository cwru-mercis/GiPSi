<GiPSiNet>
	<generalParameters>
		<verboseMode>Screen</verboseMode>
		<verboseLevel>3</verboseLevel>
		<computationalHook>false</computationalHook>
		<networkHook>false</networkHook>
		<simTime>0.01</simTime>
		<gravity>1000.0</gravity>
	</generalParameters>
	<simulationObjects>
		<simObject>
			<name>BLOOD</name>
			<type>LF</type>
			<geometries>
				<geometry>
					<geometryFile>
						<path>objects</path>
						<fileName>dumbell_4K1i.obj</fileName>
					</geometryFile>
					<textureNames>
						<textureName>SmallTGA</textureName>
					</textureNames>
				</geometry>
			</geometries>
			<visualization>
				<baseColor>
					<red>0.8</red>
					<green>0.1</green>
					<blue>0.1</blue>
					<opacity>1</opacity>
				</baseColor>
				<shader>
					<name>phong</name>
					<params>
						<param>
							<name>halfWayApprox</name>
							<value>true</value>
						</param>
						<param>
							<name>texUnitBase</name>
							<value>1</value>
						</param>
					</params>
				</shader>
			</visualization>
			<transformation>
				<rotation>
					<axisRotation>
						<axis>
							<x>1</x>
							<y>0</y>
							<z>0</z>
						</axis>
						<angle>0</angle>
					</axisRotation>
				</rotation>
				<scaling>
					<x>1</x>
					<y>1</y>
					<z>1</z>
				</scaling>
				<translation>
					<x>0</x>
					<y>0</y>
					<z>0</z>
				</translation>
			</transformation>
			<objParameters>
				<NHParameters>
					<time>0</time>
					<timeStep>0.001</timeStep>
					<numericMethod>Euler</numericMethod>
					<modelParameters>
						<LFParameters>
							<Pi>-100</Pi>
							<Po>100</Po>
							<Pfo>0</Pfo>
							<Kc>20</Kc>
							<Bc>0.01</Bc>
							<Ri>0.1</Ri>
							<Ro>0.1</Ro>
						</LFParameters>
					</modelParameters>
				</NHParameters>
			</objParameters>
		</simObject><simObject>
			<name>BIOELE</name>
			<type>CBE</type>
			<geometries>
				<geometry>
					<geometryFile>
						<path>objects</path>
						<fileName>dumbell_4K.neu</fileName>
					</geometryFile>
					<textureNames>
						<textureName>SmallTGA</textureName>
					</textureNames>
				</geometry>
			</geometries>
			<visualization>
				<baseColor>
					<red>0.1</red>
					<green>1</green>
					<blue>0.1</blue>
					<opacity>1</opacity>
				</baseColor>
				<shader>
					<name>bump</name>
					<params>
						<param>
							<name>texUnitBase</name>
							<value>1</value>
						</param>
						<param>
							<name>texUnitHeight</name>
							<value>2</value>
						</param>
					</params>
				</shader>
			</visualization>
			<transformation>
				<rotation>
					<axisRotation>
						<axis>
							<x>1</x>
							<y>0</y>
							<z>0</z>
						</axis>
					<angle>0</angle>
					</axisRotation>
				</rotation>
				<scaling>
					<x>1</x>
					<y>1</y>
					<z>1</z>
				</scaling>
				<translation>
					<x>0</x>
					<y>0</y>
					<z>0</z>
				</translation>
			</transformation>
			<objParameters>
				<NHParameters>
					<time>0</time>
					<timeStep>0.001</timeStep>
					<numericMethod>Euler</numericMethod>
					<modelParameters>
						<CBEParameters>
							<TempFreq>1</TempFreq>
							<SpVel>2</SpVel>
							<DutyCycle>5</DutyCycle>
						</CBEParameters>
					</modelParameters>
				</NHParameters>
			</objParameters>
		</simObject><simObject>
			<name>MUSCLE</name>
			<type>FEM</type>
			<geometries>
				<geometry>
					<geometryFile>
						<path>objects</path>
						<fileName>dumbell_4K.neu</fileName>
					</geometryFile>
					<textureNames>
						<textureName>SmallTGA</textureName>
					</textureNames>
				</geometry>
			</geometries>
			<visualization>
				<baseColor>
					<red>1</red>
					<green>0.2</green>
					<blue>0.2</blue>
					<opacity>0.5</opacity>
				</baseColor>
				<shader>
					<name>tissue</name>
					<params>
						<param>
							<name>texUnitBase</name>
							<value>1</value>
						</param>
						<param>
							<name>texUnitHeight</name>
							<value>2</value>
						</param>
						<param>
							<name>ambientContribution</name>
							<value>3.0</value>
						</param>
						<param>
							<name>diffuseContribution</name>
							<value>4.0</value>
						</param>
						<param>
							<name>specularContribution</name>
							<value>5.0</value>
						</param>
						<param>
							<name>glossiness</name>
							<value>6.0</value>
						</param>
						<param>
							<name>stepSize</name>
							<value>7.0</value>
						</param>
						<param>
							<name>bumpiness</name>
							<value>8.0</value>
						</param>
						<param>
							<name>opacity</name>
							<value>9.0</value>
						</param>
						<param>
							<name>displacement</name>
							<value>10.0</value>
						</param>
					</params>
				</shader>
			</visualization>
			<transformation>
				<rotation>
					<axisRotation>
						<axis>
							<x>1</x>
							<y>0</y>
							<z>0</z>
						</axis>
						<angle>0</angle>
					</axisRotation>
				</rotation>
				<scaling>
					<x>1</x>
					<y>1</y>
					<z>1</z>
				</scaling>
				<translation>
					<x>0</x>
					<y>0</y>
					<z>0</z>
				</translation>
			</transformation>
			<objParameters>
				<NHParameters>
					<time>0</time>
					<timeStep>0.001</timeStep>
					<numericMethod>Euler</numericMethod>
					<modelParameters>
						<FEMParameters>
							<Rho>0.03</Rho>
							<Mu>64.4068</Mu>
							<Lambda>3.3898</Lambda>
							<Nu>1</Nu>
							<Phi>1</Phi>
						</FEMParameters>
					</modelParameters>
				</NHParameters>
			</objParameters>
		</simObject>
	</simulationObjects>
	<connectors>
		<connector>
			<name>Connector1</name>
			<type>FEM/CBE</type>
			<object1Name>MUSCLE</object1Name>
			<object2Name>BIOELE</object2Name>
			<modelParameters>
				<FEM_CBEParameters>
					<ExStressValueXX>35</ExStressValueXX>
					<ExStressValueYY>35</ExStressValueYY>
					<ExStressValueZZ>35</ExStressValueZZ>
				</FEM_CBEParameters>
			</modelParameters>
		</connector><connector>
			<name>Connector2</name>
			<type>FEM/LF</type>
			<object1Name>MUSCLE</object1Name>
			<object2Name>BLOOD</object2Name>
			<modelParameters>
				<FEM_LFParameters>
					<corrFile>
						<path>objects</path>
						<fileName>dumbell_4K.obj.dumbell_4K1.obj.corr</fileName>
					</corrFile>
				</FEM_LFParameters>
			</modelParameters>
		</connector>
	</connectors>
	<visualization>
		<textures>
			<texture>
				<name>SmallTGA</name>
				<type>GIPSI_2D_STATIC_CLIENT</type>
				<path>TGAFiles</path>
				<fileName>TestS.tga</fileName>
			</texture>
			<texture>
				<name>MediumTGA</name>
				<type>GIPSI_2D_DYNAMIC_SERVER</type>
				<path>TGAFiles</path>
				<fileName>TestM.tga</fileName>
			</texture>
			<texture>
				<name>LargeTGA</name>
				<type>GIPSI_2D_STATIC_SERVER</type>
				<path>TGAFiles</path>
				<fileName>TestL.tga</fileName>
			</texture>
		</textures>
		<scenes>
			<scene>
				<simulationObjectNames>
					<simObjectName>ALL</simObjectName>
				</simulationObjectNames>
				<cameras>
					<camera>
						<name>camera1</name>
