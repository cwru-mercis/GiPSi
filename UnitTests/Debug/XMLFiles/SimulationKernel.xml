						<type>FREE</type>
						<position>
							<x>1</x>
							<y>1</y>
							<z>1</z>
						</position>
						<direction>
							<x>7</x>
							<y>2</y>
							<z>3</z>
						</direction>
						<color>
							<red>0.5</red>
							<green>0.5</green>
							<blue>0.5</blue>
							<opacity>0.7</opacity>
						</color>
						<cameraNames>
							<cameraName>ALL</cameraName>
						</cameraNames>
					</light>
				</lights>
			</scene>
		</scenes>
		<shaders>
			<shader>
				<name>phong</name>
				<path>.\Shaders\phong</path>
				<passes>1</passes>
			</shader>
			<shader>
				<name>bump</name>
				<path>.\Shaders\bump</path>
				<passes>1</passes>
			</shader>
			<shader>
				<name>tissue</name>
				<path>.\Shaders\tissue</path>
				<passes>1</passes>
			</shader>
		</shaders>
	</visualization>
	<collisionDAR>
		<enabled>false</enabled>
		<colDetectionMethod>Tri-Tri</colDetectionMethod>
		<colResponseMethod>Some Collision Response Method</colResponseMethod>
